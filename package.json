{
	"name": "ai-ecommerce-dashboard",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"generate:dev": "npx prisma generate",
		"prisma:studio": "dotenv -e .env.local -- pnpm prisma studio",
		"migrate:reset:dev": "dotenv -e .env.local -- pnpm prisma migrate reset",
		"migrate:push": "dotenv -e .env.local -- pnpm prisma migrate push",
		"migrate:dev": "dotenv -e .env.local -- pnpm prisma migrate dev",
		"migrate:prod": "dotenv -e .env.production -- pnpm prisma migrate deploy",
		"vercel-build": "prisma generate && prisma migrate deploy && next build",
		"postinstall": "prisma generate"
	},
	"prisma": {
		"schema": "src/db/schema.prisma"
	},
	"dependencies": {
		"@ai-sdk/amazon-bedrock": "^2.2.4",
		"@ai-sdk/react": "^1.2.5",
		"@icons-pack/react-simple-icons": "^12.3.0",
		"@neondatabase/serverless": "^1.0.0",
		"@next/env": "^15.2.4",
		"@prisma/adapter-neon": "^6.5.0",
		"@prisma/client": "^6.5.0",
		"@radix-ui/react-alert-dialog": "^1.1.6",
		"@radix-ui/react-collapsible": "^1.1.3",
		"@radix-ui/react-dialog": "^1.1.6",
		"@radix-ui/react-dropdown-menu": "^2.1.6",
		"@radix-ui/react-label": "^2.1.2",
		"@radix-ui/react-scroll-area": "^1.2.3",
		"@radix-ui/react-select": "^2.1.6",
		"@radix-ui/react-separator": "^1.1.2",
		"@radix-ui/react-slot": "^1.1.2",
		"@radix-ui/react-tooltip": "^1.1.8",
		"@types/cookie-signature": "^1.1.2",
		"ai": "^4.2.10",
		"better-auth": "^1.2.5",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cookie-signature": "^1.2.2",
		"dotenv-cli": "^8.0.0",
		"jquery": "^3.7.1",
		"lucide-react": "^0.484.0",
		"nanoid": "^5.1.5",
		"next": "15.2.4",
		"next-themes": "^0.4.6",
		"nuqs": "^2.4.1",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-icons": "^5.5.0",
		"server-only": "^0.0.1",
		"sonner": "^2.0.2",
		"tailwind-merge": "^3.0.2",
		"tw-animate-css": "^1.2.4",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"prisma": "^6.5.0",
		"tailwindcss": "^4",
		"typescript": "^5"
	}
}
